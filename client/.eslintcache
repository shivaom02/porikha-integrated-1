clearInterval(this.countDownId);\r\n      this.setState({\r\n        days: 0,\r\n        hours: 0,\r\n        minutes: 0,\r\n        seconds: 0,\r\n        expired: true\r\n      });\r\n      return;\r\n    }\r\n    this.setState({ days, hours, minutes, seconds, expired: false });\r\n  };\r\n\r\n  render() {\r\n    const { days, hours, minutes, seconds, expired } = this.state;\r\n    if (expired) {\r\n      return <div className=\"expired\">Expired :(</div>;\r\n    }\r\n    return (\r\n      <div className=\"timer\">\r\n        \r\n        <div>\r\n          {hours}\r\n          <span>h</span>\r\n        </div>\r\n        <div>\r\n          {minutes}\r\n          <span>m</span>\r\n        </div>\r\n        <div>\r\n          {seconds}\r\n          <span>s</span>\r\n        </div>\r\n      </div>\r\n    );\r\n  }\r\n}\r\nexport default Timer;\r\n","C:\\Users\\Shivam Kumar\\Desktop\\NOW\\Porikha\\CSShack1.0\\porikha\\client\\src\\components\\navbar\\Menuitems.js",[],"C:\\Users\\Shivam Kumar\\Desktop\\NOW\\porikha-int-front\\client\\src\\index.js",[],"C:\\Users\\Shivam Kumar\\Desktop\\NOW\\porikha-int-front\\client\\src\\App.js",[],"C:\\Users\\Shivam Kumar\\Desktop\\NOW\\porikha-int-front\\client\\src\\routes\\PrivateRoute.js",[],"C:\\Users\\Shivam Kumar\\Desktop\\NOW\\porikha-int-front\\client\\src\\utilsClient\\setToken.js",[],"C:\\Users\\Shivam Kumar\\Desktop\\NOW\\porikha-int-front\\client\\src\\components\\login_register\\login.js",[],"C:\\Users\\Shivam Kumar\\Desktop\\NOW\\porikha-int-front\\client\\src\\components\\login_register\\register.js",[],"C:\\Users\\Shivam Kumar\\Desktop\\NOW\\porikha-int-front\\client\\src\\context\\examContexts\\AppState.js",[],"C:\\Users\\Shivam Kumar\\Desktop\\NOW\\porikha-int-front\\client\\src\\context\\authContexts\\authContext.js",[],"C:\\Users\\Shivam Kumar\\Desktop\\NOW\\porikha-int-front\\client\\src\\context\\examContexts\\AppContext.js",[],"C:\\Users\\Shivam Kumar\\Desktop\\NOW\\porikha-int-front\\client\\src\\context\\exam