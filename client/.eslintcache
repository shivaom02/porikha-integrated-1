ents\\Timer\\Timer.js",["364"],"\r\nimport React, { Component } from \"react\";\r\nimport \"./Timer.css\";\r\nclass Timer extends Component {\r\n  constructor(props) {\r\n    super(props);\r\n    this.countDownId = null;\r\n    this.state = {\r\n      days: 0,\r\n      hours: 0,\r\n      minutes: 0,\r\n      seconds: 0,\r\n      expired: false\r\n    };\r\n  }\r\n\r\n  componentDidMount() {\r\n    this.countDownId = setInterval(this.timerInit, 1000);\r\n  }\r\n\r\n  componentWillUnmount() {\r\n    if (this.countDownId) {\r\n      clearInterval(this.countDownId);\r\n    }\r\n  }\r\n\r\n  timerInit = () => {\r\n    const { startDate } = this.props;\r\n    console.log(startDate);\r\n    const now = new Date().getTime();\r\n    if (!startDate) {\r\n      this.setState({ expired: true });\r\n      return;\r\n    }\r\n    const countDownStartDate = new Date(startDate).getTime();\r\n    const distance = countDownStartDate - now;\r\n    const days = Math.floor(distance / (1000 * 60 * 60 * 24));\r\n    const hours = Math.floor(\r\n      (distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60)\r\n    );\r\n    const minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));\r\n    const seconds = Math.floor((distance % (1000 * 60)) / 1000);\r\n\r\n    // For countdown is finished\r\n    if (distance < 0) {\r\n      clearInterval(this.countDownId);\r\n      this.setState({\r\n        days: 0,\r\n        hours: 0,\r\n        minutes: 0,\r\n        seconds: 0,\r\n        expired: true\r\n      });\r\n      return;\r\n    }\r\n    this.setState({ days, hours, minutes, seconds, expired: false });\r\n  };\r\n\r\n  render() {\r\n    const { days, hours, minutes, seconds, expired } = this.state;\r\n    if (expired) {\r\n      return <div className=\"expired\">Expired :(</div>;\r\n    }\r\n    return (\r\n      <div className=\"timer\">\r\n        \r\n        <div>\r\n          {hours}\r\n          <span>h</span>\r\n        </div>\r\n        <div>\r\n          {minutes}\r\n          <span>m</span>\r\n        </div>\r\n        <div>\r\n          {seconds}\r\n          <span>s</span>\r\n        </div>\r\n      </div>\r\n    );\r\n  }\r\n}\r\nexport default Timer;\r\n","C:\\Users\\Shivam Kumar\\Desktop\\NOW\\Porikha\\CSShack1.0\\porikha\\client\\src\\components\\navbar\\Menuitems.js",[],"C:\\Users\\Shivam Kumar\\Desktop\\NOW\\porikha-int-front\\client\\src\\index.js",[],"C:\\Users\\Shivam Kumar\\Desktop\\NOW\\porikha-int-front\\client\\src\\App.js",[],"C:\\Users\\Shivam Kumar\\Desktop\\NOW\\porikha-int-front\\client\\src\\routes\\PrivateRoute.js",[],"C:\\Users\\Shivam Kumar\\Desktop\\NOW\\porikha-int-front\\client\\src\\utilsClient\\setToken.js",[],"C:\\Users\\Shivam Kumar\\Desktop\\NOW\\porikha-int-front\\client\\src\\components\\login_register\\login.js",[],"C:\\Users\\Shivam Kumar\\Desktop\\NOW\\porikha-int-front\\client\\src\\components\\login_register\\register.js",[],"C:\\Users\\Shivam Kumar\\Desktop\\NOW\\porikha-int-front\\client\\src\\context\\examContexts\\AppState.js",[],"C:\\Users\\Shivam Kumar\\Desktop\\NOW\\porikha-int-front\\client\\src\\context\\authContexts\\authContext.js",[],"C:\\Users\\Shivam Kumar\\Desktop\\NOW\\porikha-int-front\\client\\src\\context\\examContexts\\AppContext.js",[],"C:\\Users\\Shivam Kumar\\Desktop\\NOW\\porikha-int-front\\client\\src\\context\\examContexts\\AppReducer.js",["365"],"C:\\Users\\Shivam Kumar\\Desktop\\NOW\\porikha-int-front\\client\\src\\context\\types.js",[],"C:\\Users\\Shivam Kumar\\Desktop\\NOW\\porikha-int-front\\client\\src\\context\\authContexts\\authState.js",[],"C:\\Users\\Shivam Kumar\\Desktop\\NOW\\porikha-int-front\\client\\src\\context\\authContexts\\authReducer.js",[],"C:\\Users\\Shivam Kumar\\Desktop\\NOW\\porikha-int-front\\client\\src\\context\\studentContexts\\studentState.js",[],"C:\\Users\\Shivam Kumar\\Desktop\\NOW\\porikha-int-front\\client\\src\\context\\studentContexts\\studentReducer.js",["366"],"C:\\Users\\Shivam Kumar\\Desktop\\NOW\\porikha-int-front\\client\\src\\